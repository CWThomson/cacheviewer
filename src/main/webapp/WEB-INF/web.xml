<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

    <display-name>CacheViewer</display-name>
    <description>Coherence Cache Viewer</description>

    <servlet>
		<servlet-name>Extractor</servlet-name>
		<servlet-class>uk.co.christhomson.coherence.viewer.web.ExtractorServlet</servlet-class>
	</servlet>
	
    <servlet>
		<servlet-name>BulkLoader</servlet-name>
		<servlet-class>uk.co.christhomson.coherence.viewer.web.BulkLoader</servlet-class>
	</servlet>
	
    <servlet>
		<servlet-name>CacheDump</servlet-name>
		<servlet-class>uk.co.christhomson.coherence.viewer.web.CacheDump</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Extractor</servlet-name>
		<url-pattern>/Extractor</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>BulkLoader</servlet-name>
		<url-pattern>/BulkLoader</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CacheDump</servlet-name>
		<url-pattern>/CacheDump</url-pattern>
	</servlet-mapping>
	
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
</web-app>